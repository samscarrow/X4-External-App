{
  "author": "Mycu",
  "name": "x4_external_app",
  "version": "3.3.0",
  "bin": "server.js",
  "pkg": {
    "assets": [
      "node_modules/**/*",
      "dist/**/*"
    ],
    "targets": [
      "node16-win-x64",
      "node16-linux-x64"
    ],
    "outputPath": "dist"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js --ignore-pattern node_modules/ --ignore-pattern dist/ --ignore-pattern dev-dist/",
    "lint:fix": "eslint . --ext .js --fix --ignore-pattern node_modules/ --ignore-pattern dist/ --ignore-pattern dev-dist/",
    "package:linux": "npm run build && pkg . --targets node16-linux-x64 --output dist/x4_external_app_linux",
    "package:win": "npm run build && pkg . --targets node16-win-x64 --output dist/x4_external_app.exe"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@notionhq/client": "^5.3.0",
    "@vueform/toggle": "^2.0.1",
    "better-sqlite3": "^11.7.0",
    "body-parser": "^2.2.0",
    "chalk": "4.1.2",
    "chokidar": "^3.5.2",
    "compression": "^1.8.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "fast-xml-parser": "^4.3.2",
    "github-version-checker": "^2.2.0",
    "local-ipv4-address": "^0.0.2",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "node-cron": "^3.0.3",
    "portfinder": "^1.0.28",
    "sax": "^1.4.1",
    "vue": "^3.2.25",
    "vue-i18n": "^11.1.5",
    "vue3-perfect-scrollbar": "^2.0.0",
    "vuedraggable": "^4.1.0",
    "vuex": "^4.0.2",
    "xml-stream": "^0.4.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^9.39.0",
    "sass": "^1.87.0",
    "vite": "^7.1.11",
    "vite-plugin-pwa": "^1.1.0"
  }
}
